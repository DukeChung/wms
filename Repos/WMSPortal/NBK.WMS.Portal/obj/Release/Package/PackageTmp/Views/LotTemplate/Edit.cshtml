@section Styles {
    @Styles.Render("~/Content/plugins/iCheck/iCheckStyles")
}

@section Scripts {
    @Scripts.Render("~/plugins/iCheck")
    <script src="~/Scripts/Utility/Common.js"></script>
    <script type="text/javascript">

        $(document).ready(function() {
            $('.i-checks').iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green'
            });

            $("#lotTemplateForm").validate({
                ignore: [],
                rules: {
                    Descr: {
                        required: true,
                        maxlength: 64
                    },
                }
            });
        });

        function LotTemplateSave()
        {
            if ($("#lotTemplateForm").valid()) {
                $.ajax({
                    url: "/LotTemplate/SaveLotTemplate",
                    type: "POST",
                    data: $("#lotTemplateForm").serializeObject(),
                    success: function (data) {
                        if (data.success) {

                            window.location.href = "/LotTemplate/Index";
                        }
                        else {
                            msgErro(data.message);
                        }
                    },
                    error: function () {
                        msgErro("操作失败");
                    }
                });
            }
        }
    </script>
}

