                <div class="col-sm-1">
                    入库备注:
                </div>
                <div class="col-sm-5">
                    <div class="row">
                        <div class="col-sm-12">
                            {{PurchaseDescr}}
                        </div>
                        <div class="col-sm-12 hr-line-dashed" style="margin-top: 5px;"> </div>

                    </div>
                </div>
            </div>

            <div class="form-group ">
