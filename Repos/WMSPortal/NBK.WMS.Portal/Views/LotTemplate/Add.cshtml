            //    }
            //});

        });

        function LotTemplateSave() {
            if ($("#lotTemplateForm").valid()) {
                $.ajax({
                    url: "/LotTemplate/SaveLotTemplate?actionName=Add",
                    type: "POST",
                    data: $("#lotTemplateForm").serializeObject(),
                    success: function (data) {
                        if (data.success) {

                            window.location.href = "/LotTemplate/Index";
                        } else {
                            msgErro(data.message);
                        }
                    },
                    error: function () {
                        msgErro("操作失败");
                    }
                });
            }
        }


    </script>
}

