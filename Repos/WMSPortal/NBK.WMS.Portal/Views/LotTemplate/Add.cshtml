    <script src="~/Scripts/Utility/Common.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {
            $('.i-checks').iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green'
            });
            
            $("#lotTemplateForm").validate({
                ignore: [],
                rules: {
                    LotCode: {
                        required: true,
                        maxlength: 16
                    },
                    Descr: {
                        required: true,
                        maxlength: 64
                    },
                }
            });

